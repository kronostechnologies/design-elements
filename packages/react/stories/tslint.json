{
    "extends": ["../tslint.json"],
    "rules": {
        "linebreak-style": [false],
        "variable-name": {
            "options": ["ban-keywords", "check-format", "allow-pascal-case"]
        },
        "quotemark": {
            "options": ["single", "jsx-double"]
        },
        "jsx-no-lambda": [false],
        "jsx-no-multiline-js": [false],
        "jsx-boolean-value": [false],
        "no-console": false,
        "no-default-export": false
    }
}