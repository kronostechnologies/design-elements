{
  "name": "@equisoft/design-elements-react",
  "version": "2.0.3-alpha.48",
  "description": "React implementation of the Equisoft design system.",
  "license": "MIT",
  "main": "dist/bundle.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "webpack --config ./webpack/webpack.config.prod.js",
    "prepublishOnly": "yarn build",
    "start": "webpack --config ./webpack/webpack.config.dev.js",
    "test": "cross-env NODE_ICU_DATA='node_modules/full-icu' jest",
    "test:ci": "JEST_JUNIT_OUTPUT='build/tests/jest/junit.xml' cross-env NODE_ICU_DATA='node_modules/full-icu' jest --max-workers=2 --reporters=default --reporters=jest-junit",
    "test:unit": "cross-env NODE_ICU_DATA='node_modules/full-icu' jest --config=jest.config.js",
    "lint": "yarn tslint && yarn tslint:tests && yarn stylelint",
    "lint:ci": "mkdir -p build/lint/ts build/lint/style && yarn tslint:ci && yarn tslint:tests:ci && yarn stylelint:ci",
    "tslint": "tslint -p tsconfig.json \"src/**/*.{ts,tsx}\" -e \"src/**/*.test.{ts,tsx}\" -e \"src/test-utils/*.{ts,tsx}\"",
    "tslint:ci": "yarn tslint --format junit -o build/lint/ts/junit.xml",
    "tslint:tests": "tslint -p test/tsconfig.json -c test/tslint.json \"src/**/*.test.{ts,tsx}\" \"src/test-utils/*.{ts,tsx}\"",
    "tslint:tests:ci": "yarn tslint:tests --format junit -o build/lint/ts/tests.xml",
    "stylelint": "stylelint \"./src/**/*.{ts,tsx}\"",
    "stylelint:ci": "stylelint --custom-formatter node_modules/stylelint-junit-formatter \"./src/**/*.{ts,tsx}\" > build/lint/style/junit.xml",
    "test-update": "cross-env NODE_ICU_DATA='node_modules/full-icu' jest -u"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kronostechnologies/design-elements.git"
  },
  "bugs": {
    "url": "https://github.com/kronostechnologies/design-elements/issues"
  },
  "dependencies": {
    "date-fns": "^2.14.0",
    "feather-icons": "^4.21.0",
    "prop-types": "^15.7.2",
    "react-datepicker": "^3.0.0",
    "react-is": "~16.13.1",
    "react-modal": "^3.11.2",
    "react-popper-tooltip": "^2.10.1",
    "react-shadow": "^18.4.2",
    "react-swipeable": "~5.5.0",
    "react-table": "^7.1.0",
    "uuid": "^3.3.3"
  },
  "peerDependencies": {
    "i18next": "^19.4.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.1.2",
    "styled-components": ">= 4"
  },
  "devDependencies": {
    "@equisoft/stylelint-config-styled-components": "^0.2.0",
    "@equisoft/typescript-config": "^2.0.0",
    "@svgr/webpack": "^4.3.2",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/cheerio": "^0.22.22",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/feather-icons": "^4.7.0",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.149",
    "@types/react": "~16.14.0",
    "@types/react-datepicker": "^2.9.4",
    "@types/react-dom": "~16.9.9",
    "@types/react-modal": "^3.10.6",
    "@types/react-router-dom": "~5.1.3",
    "@types/react-table": "^7.0.19",
    "@types/react-test-renderer": "^16.9.0",
    "@types/styled-components": "^5.1.3",
    "@types/testing-library__react-hooks": "^3.4.1",
    "@types/uuid": "^3.4.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "~3.6.1",
    "expect": "^26.6.2",
    "full-icu": "^1.3.1",
    "i18next": "^19.4.4",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-styled-components": "^7.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "react": "~16.14.0",
    "react-docgen-typescript-loader": "~3.7.2",
    "react-dom": "~16.14.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "~16.14.0",
    "sass": "~1.26.10",
    "sass-loader": "~10.0.1",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.1",
    "stylelint-junit-formatter": "^0.2.2",
    "to-string-loader": "~1.1.6",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.11",
    "tsconfig-paths-webpack-plugin": "~3.2.0",
    "typescript": "^4.0.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-merge": "^4.2.2"
  },
  "jest-junit": {
    "outputDirectory": "build/tests"
  }
}
